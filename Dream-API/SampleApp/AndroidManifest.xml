         <activity
            android:name="com.df.ui.MainActivity"
            android:screenOrientation="portrait" >
        </activity>
         <activity
            android:name="com.df.ui.ToDoDemoActivity"
            android:screenOrientation="portrait" >
        </activity>
         <activity
            android:name="com.df.ui.ChooseFileDemoActivity"
            android:screenOrientation="portrait" >
        </activity>
         <activity
            android:name="com.df.ui.RemoteFileDemo"
            android:screenOrientation="portrait" >
